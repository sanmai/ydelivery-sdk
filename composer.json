{
    "name": "sanmai/ydelivery-sdk",
    "type": "library",
    "description": "Яндекс.Доставка PHP SDK, библиотека для работы с API Яндекс.Доставки",
    "keywords": [
        "Яндекс",
        "Yandex",
        "delivery",
        "служба доставки"
    ],
    "homepage": "https://ydelivery-sdk.readthedocs.io/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com",
            "homepage": "https://www.alexeykopytko.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3 || ^7.0",
        "jms/serializer": "^3.9",
        "psr/log": "^1.0",
        "sanmai/json-serializer": "^0.2.2",
        "sanmai/pipeline": "^5",
        "sanmai/sdk-common": "^0.4.4",
        "sanmai/version-info": "^0.2.0",
        "symfony/http-foundation": "^4.4.7 || ^5.0.7"
    },
    "conflict": {
        "doctrine/annotations": "<1.10.3",
        "doctrine/lexer": "<1.2.0"
    },
    "require-dev": {
        "doctrine/cache": "^1.10.2",
        "ergebnis/composer-normalize": "^2.8.2",
        "friendsofphp/php-cs-fixer": "^2.16.4",
        "gamez/psr-testlogger": "^3.0",
        "illuminate/support": "^5.2",
        "infection/infection": ">=0.16",
        "laravel/framework": "^5.8",
        "phan/phan": ">1.3.0",
        "php-coveralls/php-coveralls": "^2.4.1",
        "phpstan/phpstan": ">=0.10",
        "phpunit/phpunit": "^9.4.1",
        "vimeo/psalm": "^3.18.2"
    },
    "suggest": {
        "doctrine/cache": "For metadata caching",
        "monolog/monolog": "For advanced logging and debugging",
        "sanmai/pindx": "For postal code validation"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "YDeliverySDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\YDeliverySDK\\": "tests/",
            "CommonSDK\\Tests\\Common\\": "vendor/sanmai/sdk-common/tests/Common/"
        }
    },
    "support": {
        "issues": "https://github.com/sanmai/ydelivery-sdk/issues"
    }
}
