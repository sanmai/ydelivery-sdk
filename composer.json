{
    "name": "sanmai/ydelivery-sdk",
    "type": "library",
    "description": "Яндекс.Доставка PHP SDK, библиотека для работы с API Яндекс.Доставки",
    "keywords": [
        "Яндекс",
        "Yandex",
        "delivery",
        "служба доставки"
    ],
    "homepage": "https://ydelivery-sdk.readthedocs.io/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com",
            "homepage": "https://www.alexeykopytko.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-SimpleXML": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "guzzlehttp/guzzle": "^6.3 || ^7.0",
        "jms/serializer": "^3.9",
        "psr/log": "^1.0",
        "sanmai/json-serializer": "^0.1.0",
        "sanmai/pipeline": "^3 || ^5",
        "symfony/http-foundation": "^4 || ^5.0.7"
    },
    "conflict": {
        "doctrine/annotations": "<1.10.3"
    },
    "require-dev": {
        "doctrine/cache": "^1.10",
        "friendsofphp/php-cs-fixer": "^2.14",
        "gamez/psr-testlogger": "^3.0",
        "illuminate/support": "^5.2",
        "infection/infection": ">=0.16",
        "laravel/framework": "^5.8",
        "phan/phan": "^1 <1.3 || >1.3.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": ">=0.10",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": ">=3.0.16"
    },
    "suggest": {
        "doctrine/cache": "For metadata caching",
        "monolog/monolog": "For advanced logging and debugging",
        "sanmai/pindx": "For postal code validation"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v0.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "YDeliverySDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\YDeliverySDK\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/sanmai/ydelivery-sdk/issues"
    }
}
